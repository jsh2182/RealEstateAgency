
@{
    ViewBag.Title = "مدیریت فایلهای مسکن";
    Layout = Tools.GetLayoutName();
}
@using Kendo.Mvc.UI;
<div class="container" style="width:100%; max-width:100%">
    <div class="row" style="margin-top:10px">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h4 class="panel-title">جستجوی اطلاعات </h4>
                <span class="pull-right clickable" id="SearchPanelClickable"><i class="glyphicon glyphicon-chevron-down"></i></span>
            </div>
            <div class="panel-body" id="panelSearch">
                <div class="row" style="margin-top:10px">
                    <form id="frmSearch">
                        <div class="tab_right">
                            <ul class="nav nav-tabs">
                                <li class="active"><a href="#tabSearchBaseInfo" data-toggle="tab" aria-expanded="true">اطلاعات اولیه</a></li>
                                <li class=""><a href="#tabSearchAddressInfo" data-toggle="tab" aria-expanded="false">اطلاعات آدرس</a></li>
                                <li class=""><a href="#tabOtherInfo" data-toggle="tab" aria-expanded="false">سایر اطلاعات</a></li>
                                <li class=""><a href="#tabCheckInfo" data-toggle="tab" aria-expanded="false">اطلاعات چک لیستی</a></li>
                            </ul>
                            <div class="tab-content">
                                <div class="tab-pane active" id="tabSearchBaseInfo">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <label class="labelClass">مالک:‏</label>
                                            <input type="text" class="form-control" name="PersonName" id="PersonNameSearch" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">کد فایل:‏</label>
                                            <input type="text" class="form-control" name="FileCode" id="FileCodeSearch" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">تعداد اتاق</label>
                                            <input type="text" class="form-control" name="RoomCount" id="RoomCountSearch" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">توضیحات:‏</label>
                                            <input type="text" name="FileDescription" id="FileDescriptionSearch" class="form-control" />
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-3">
                                            <label class="labelClass">نوع فایل:‏</label>
                                            @(Html.Kendo().ComboBox()
                                                  .Name("FileType")
                                                  .HtmlAttributes(new { @id = "FileTypeSearch" })
                                                  .AutoBind(true)
                                                  .DataTextField("Text")
                                                  .DataValueField("Value")
                                               .Filter(FilterType.StartsWith)
                                               .Events(e => e.Select("OnFileTypeChange"))
                                                 .DataSource(ds => ds.Read(r => r.Action("LoadFileTypes", "EstateFile")))
                                            )
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">نوع ملک:‏</label>
                                            @(Html.Kendo().ComboBox()
                                                   .Name("EstateType")
                                                   .HtmlAttributes(new { @id = "EstateTypeSearch" })
                                                   .AutoBind(true)
                                                   .DataTextField("Text")
                                                   .DataValueField("Value")
                                                   .Filter(FilterType.StartsWith)
                                                   //.Events(e => e.Select("OnEstateTypeChanged"))
                                                   .DataSource(ds => ds.Read(r => r.Action("LoadEstateTypes", "EstateFile")))
                                            )
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">قیمت واحد:‏</label>
                                            <input type="number" name="UnitPrice" id="UnitPriceSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">قیمت کل:‏</label>
                                            <input type="number" name="TotalPrice" id="TotalPriceSearch" class="form-control" />
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-3">
                                            <label class="labelClass">جهت نما:‏</label>
                                            @(Html.Kendo().ComboBox()
                                                          .Name("BuildingFacade")
                                                          .HtmlAttributes(new { @id = "BuildingFacadeSearch" })
                                                          .DataTextField("Text")
                                                          .DataValueField("Value")
                                                .Filter(FilterType.StartsWith)
                                                          .DataSource(ds => ds.Read(r => r.Action("LoadBuildingFacade", "EstateFile")))
                                            )
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">منبع اطلاعات:‏‏</label>
                                            @(Html.Kendo().ComboBox()
                                                .Name("InformationSource")
                                                .HtmlAttributes(new { @id = "InformationSourceSearch" })
                                                .DataTextField("Text")
                                                .DataValueField("Value")
                                                .Filter(FilterType.StartsWith)
                                                .DataSource(ds => ds.Read(r => r.Action("LoadInformationSource", "EstateFile")))
                                            )
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">سرمایش:‏‏</label>
                                            @(Html.Kendo().ComboBox()
                                                .Name("Cooling")
                                                .HtmlAttributes(new { @id = "CoolingSearch" })
                                                .DataTextField("Text")
                                                .DataValueField("Value")
                                                .Filter(FilterType.StartsWith)
                                                .DataSource(ds => ds.Read(r => r.Action("LoadCoolings", "EstateFile")))
                                            )
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">گرمایش:‏‏</label>
                                            @(Html.Kendo().ComboBox()
                                                .Name("Heating")
                                                .HtmlAttributes(new { @id = "HeatingSearch" })
                                                .DataTextField("Text")
                                                .DataValueField("Value")
                                                .Filter(FilterType.StartsWith)
                                                .DataSource(ds => ds.Read(r => r.Action("LoadHeatings", "EstateFile")))
                                            )
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-3">
                                            <label class="labelClass">وضعیت:‏‏</label>
                                            @(Html.Kendo().ComboBox()
                                                .Name("Status")
                                                .HtmlAttributes(new { @id = "StatusSearch" })
                                                .DataTextField("Text")
                                                .DataValueField("Value")
                                                .Filter(FilterType.StartsWith)
                                                .DataSource(ds => ds.Read(r => r.Action("LoadStatus", "EstateFile")))
                                            )
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">وضعیت سند:‏‏</label>
                                            @(Html.Kendo().ComboBox()
                                                .Name("OwnershipDocumentStatus")
                                                .HtmlAttributes(new { @id = "OwnershipDocumentStatusSearch" })
                                                .DataTextField("Text")
                                                .DataValueField("Value")
                                                .Filter(FilterType.StartsWith)
                                                .DataSource(ds => ds.Read(r => r.Action("LoadOwnershipDocumentStatus", "EstateFile")))
                                            )
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">سرویس بهداشتی:‏‏</label>
                                            @(Html.Kendo().ComboBox()
                                                .Name("RestRoom")
                                                .HtmlAttributes(new { @id = "RestRoomSearch" })
                                                .DataTextField("Text")
                                                .DataValueField("Value")
                                                .Filter(FilterType.StartsWith)
                                                .DataSource(ds => ds.Read(r => r.Action("LoadRestrooms", "EstateFile")))
                                            )
                                        </div>
                                    </div>
                                </div>
                                <div id="tabSearchAddressInfo" class="tab-pane fade">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <label class="labelClass">استان:‏</label>
                                            @(Html.Kendo().ComboBox()
                                                .Name("ProvinceID")
                                                .HtmlAttributes(new { @id = "ProvinceIDSearch" })
                                                .AutoBind(true)
                                                .Placeholder("")
                                                .DataTextField("Text")
                                                .DataValueField("Value")
                                                .Filter(FilterType.StartsWith)
                                                .DataSource(ds => ds.Read(r => r.Action("LoadProvinceList", "Province")).ServerFiltering(false))
                                            )
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">شهر:‏</label>
                                            @(Html.Kendo().ComboBox()
                                                .Name("CityID")
                                                .HtmlAttributes(new { @id = "CityIDSearch" })
                                                .AutoBind(false)
                                                .Placeholder("")
                                                .DataTextField("Text")
                                                .DataValueField("Value")
                                                .Filter(FilterType.StartsWith)
                                                .CascadeFrom("ProvinceIDSearch")

                                                        .DataSource(ds => ds.Read(r => r.Action("LoadCityList", "City").Data("GetProvinceIDForSearch"))
                                                                  .ServerFiltering(true))
                                            )
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">منطقه:‏</label>
                                            @(Html.Kendo().ComboBox()
                                                   .Name("ZoneID")
                                                   .HtmlAttributes(new { @id = "ZoneIDSearch" })
                                                   .AutoBind(false)
                                                   .Placeholder("")
                                                   .DataTextField("Text")
                                                   .DataValueField("Value")
                                                   .Filter(FilterType.StartsWith)
                                                   .CascadeFrom("CityIDSearch")
                                                   .DataSource(ds => ds.Read(r => r.Action("LoadZoneList", "Zone").Data("GetCityIDForSearch")).ServerFiltering(true))
                                            )
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-3">
                                            <label class="labelClass">خیابان اصلی:‏</label>
                                            <input type="text" class="form-control" name="AddressStreet" id="AddressStreetSearch" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">کوچه:‏</label>
                                            <input type="text" class="form-control" name="AddressAlley" id="AddressAlleySearch" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">پلاک:‏</label>
                                            <input type="text" class="form-control" name="AddressNumber" id="AddressNumberSearch" />
                                        </div>
                                    </div>
                                </div>
                                <div id="tabOtherInfo" class="tab-pane fade">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <label class="labelClass">تعداد پارکینگ:‏</label>
                                            <input type="number" name="ParkingCount" id="ParkingCountSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment villa realEstate">
                                            <label class="labelClass">سن بنا:‏</label>
                                            <input type="number" name="BuildingAge" id="BuildingAgeSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment villa">
                                            <label class="labelClass">تعداد خواب مستر:‏</label>
                                            <input type="number" name="MasterRoomCount" id="MasterRoomCountSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment villa">
                                            <label class="labelClass">متراژ انباری:‏</label>
                                            <input type="number" name="StoreRoomArea" id="StoreRoomAreaSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 villa appartment">
                                            <label class="labelClass">تعداد سرویس بهداشتی:‏</label>
                                            <input type="number" name="RestRoomCount" id="RestRoomCountSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment villa realEstate">
                                            <label class="labelClass">واحد در هر طبقه:‏</label>
                                            <input type="number" name="UnitCountInFloor" id="UnitCountInFloorSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment">
                                            <label class="labelClass">تعداد لاین آسانسور:‏</label>
                                            <input type="number" name="ElevatorCount" id="ElevatorCountSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment">
                                            <label class="labelClass">برند آسانسور:‏</label>
                                            <input type="number" name="ElevatorBrand" id="ElevatorBrandSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment">
                                            <label class="labelClass">متراژ تراس:‏</label>
                                            <input type="number" name="TerraceArea" id="TerraceAreaSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment">
                                            <label class="labelClass">جنس نما:‏‏</label>
                                            <input type="text" name="FacadeMaterial" id="FacadeMaterialSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment">
                                            <label class="labelClass">کفپوش:‏‏</label>
                                            <input type="text" name="Flooring" id="FlooringSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment">
                                            <label class="labelClass">ارتفاع سقف لابی:‏</label>
                                            <input type="number" name="LobbyCeilingHeight" id="LobbyCeilingHeightSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment">
                                            <label class="labelClass">ارتفاع سقف واحد:‏</label>
                                            <input type="number" name="UnitCeilingHeight" id="UnitCeilingHeightSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 realEstate villa">
                                            <label class="labelClass">عرض گذر:‏</label>
                                            <input type="number" name="PassWidth" id="PassWidthSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 villa">
                                            <label class="labelClass">متراژ حیاط:‏</label>
                                            <input type="number" name="YardAread" id="YardAreadSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 commercial villa">
                                            <label class="labelClass">متراژ زیرزمین:‏</label>
                                            <input type="number" name="BasementArea" id="BasementAreaSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 villa">
                                            <label class="labelClass">متراژ طبقه اول:‏</label>
                                            <input type="number" name="FirstFloorArea" id="FirstFloorAreaSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 villa">
                                            <label class="labelClass">متراژ طبقه دوم:‏</label>
                                            <input type="number" name="SecondFloorArea" id="SecondFloorAreaSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 villa">
                                            <label class="labelClass">متراژ طبقه سوم:‏</label>
                                            <input type="number" name="ThirdFloorArea" id="ThirdFloorAreaSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 villa">
                                            <label class="labelClass">متراژ همکف:‏</label>
                                            <input type="number" name="GroundFloorArea" id="GroundFloorAreaSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 villa">
                                            <label class="labelClass">پهنه طرح تفصیلی:‏</label>
                                            <input type="text" name="DetailedPlanArea" id="DetailedPlanAreaSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 villa">
                                            <label class="labelClass">تعداد آشپزخانه:‏</label>
                                            <input type="number" name="KitchenCount" id="KitchenCountSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 commercial">
                                            <label class="labelClass">متراژ تابلو:‏</label>
                                            <input type="number" name="AreaForBoard" id="AreaForBoardSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 appartment">
                                            <label class="labelClass">کل واحدها:‏</label>
                                            <input type="number" name="AllUnitsCount" id="AllUnitsCountSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 realEstate">
                                            <label class="labelClass">متراژ تجاری:‏</label>
                                            <input type="number" name="AreaForCommerce" id="AreaForCommerceSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3 office">
                                            <label class="labelClass">تعداد خط تلفن:‏</label>
                                            <input type="number" name="PhoneLineCount" id="PhoneLineCountSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">طبقات ساختمان:‏</label>
                                            <input type="number" name="BuildingFloorCount" id="BuildingFloorCountSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">طبقه آپارتمان:‏</label>
                                            <input type="number" name="FloorNumber" id="FloorNumberSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">تلفن:‏</label>
                                            <input type="number" name="EstatePhoneNumber" id="EstatePhoneNumberSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">متراژ کل:‏</label>
                                            <input type="number" name="Area" id="AreaSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">بر ساختمان:‏</label>
                                            <input type="number" name="Width" id="WidthSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">زیربنا:‏</label>
                                            <input type="number" name="BuiltUpArea" id="BuiltUpAreaSearch" class="form-control" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="labelClass">آشپزخانه:‏</label>
                                            <input type="text" name="Kitchen" id="KitchenSearch" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                                <div id="tabCheckInfo" class="tab-pane fade">
                                    <div class="row">
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasStoreRoomSearch" name="HasStoreRoom" value="false" class="otherInfo" /><label style="margin-right:5px;">انباری</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasElevatorSearch" name="HasElevator" value="false" class="otherInfo" /><label style="margin-right:5px;">آسانسور</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasRemoteDoorSearch" name="HasRemoteDoor" value="false" class="otherInfo" /><label style="margin-right:5px;">درب ریموت</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasBackyardSearch" name="HasBackyard" value="false" class="otherInfo" /><label style="margin-right:5px;">حیاط خلوت</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasVideoDoorPhoneSearch" name="HasVideoDoorPhone" value="false" class="otherInfo" /><label style="margin-right:5px;">آیفون تصویری</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="NeedToRebuildSearch" name="NeedToRebuild" value="false" class="otherInfo" /><label style="margin-right:5px;">نیازمند بازسازی</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="IntegratedSalonSearch" name="IntegratedSalon" value="false" class="otherInfo" /><label style="margin-right:5px;">سالن یک تکه</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="SeparateSuiteSearch" name="SeparateSuite" value="false" class="otherInfo" /><label style="margin-right:5px;">سوئیت مجزا</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasLaundryRoomSearch" name="HasLaundryRoom" value="false" class="otherInfo" /><label style="margin-right:5px;">لاندری</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="ResidentOwnerSearch" name="ResidentOwner" value="false" class="otherInfo" /><label style="margin-right:5px;">مالک ساکن</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="IsRealEstateSearch" name="IsRealEstate" value="false" class="otherInfo" /><label style="margin-right:5px;">مستغلات</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasPoolSearch" name="HasPool" value="false" class="otherInfo" /><label style="margin-right:5px;">استخر</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasSaunaSearch" name="HasSauna" value="false" class="otherInfo" /><label style="margin-right:5px;">سونا</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasJacuzziSearch" name="HasJacuzzi" value="false" class="otherInfo" /><label style="margin-right:5px;">جکوزی</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasRoofGardenSearch" name="HasRoofGarden" value="false" class="otherInfo" /><label style="margin-right:5px;">روف گاردن</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasConferenceHallSearch" name="HasConferenceHall" value="false" class="otherInfo" /><label style="margin-right:5px;">سالن اجتماعات</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasLobbySearch" name="HasLobby" value="false" class="otherInfo" /><label style="margin-right:5px;">لابی</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
                                            <input type="checkbox" id="HasGymSearch" name="HasGym" value="false" class="otherInfo" /><label style="margin-right:5px;">سالن جیم</label>
                                        </div>
                                        <div class="col-md-2" style="margin-right:15px;">
